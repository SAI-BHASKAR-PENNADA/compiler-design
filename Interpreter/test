class person:
    integer a

    def whoareyou():
        print "i am a person"
    enddef

    def personmethod():
        print "another method"
    enddef
classend

class employee derived person:
    def whoareyou():
        print "i am an employee"
    enddef
classend

ravi isa person

ravi.whoareyou()
ravi.personmethod()

sai isa employee

sai.whoareyou()
sai.personmethod()
